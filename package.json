    "firebase-admin": "^11.0.0 || ^12.0.0",
    "moment": "^2.24.0",
    "qs": "^6.7.0",
    "ws": "^8.16.0",
    "xhr2": "^0.2.1"
  },
  "peerDependenciesMeta": {
    "firebase-admin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arbel/node-xhr2": "^0.2.0",
    "@types/atob": "^2.1.4",
    "@types/axios": "^0.14.0",
    "@types/btoa": "^1.2.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.3",
