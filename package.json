{
  "name": "@arbel/firebase-orm",
  "version": "1.9.44",
  "description": "Firestore Orm",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
